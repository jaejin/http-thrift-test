<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>UserServlet</servlet-name>
        <display-name>user</display-name>
        <description>user</description>
        <servlet-class>client.UserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserServerServlet</servlet-name>
        <display-name>userServer</display-name>
        <description>userServer thrift</description>
        <servlet-class>server.UserServerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HttpUserServlet</servlet-name>
        <display-name>httpUserItem</display-name>
        <description>httpUserItem</description>
        <servlet-class>client.HttpUserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserServlet</servlet-name>
        <url-pattern>/userItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserServerServlet</servlet-name>
        <url-pattern>/userItemServer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HttpUserServlet</servlet-name>
        <url-pattern>/httpUserItem</url-pattern>
    </servlet-mapping>
</web-app>
